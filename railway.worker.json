{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "src/LeadFlowAI.Worker/Dockerfile"
  },
  "deploy": {
    "startCommand": "dotnet LeadFlowAI.Worker.dll",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
